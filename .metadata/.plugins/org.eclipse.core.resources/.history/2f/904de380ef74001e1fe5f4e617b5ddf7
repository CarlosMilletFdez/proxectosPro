package compilador41_49;

import java.io.IOException;

public class compi45 {

	public static void main(String[] args) throws IOException {

		char letra;
		System.out.print(">");
		letra = (char) System.in.read();

		while (letra != '\n') {
			System.out.print((letra)+1);
			letra = (char) System.in.read();
		}
	}
}
