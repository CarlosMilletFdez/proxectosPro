package compilador41_49;

import java.io.IOException;

public class compi46 {
	
	public static void main(String[] args) throws IOException {

		char letra;
		letra = (char) System.in.read();
		letra = (char) (letra -3);
		System.out.print(">");
				
		while (letra != '\r') {

			letra = (char) (letra + 3);
			System.out.print(letra);
			letra = (char) System.in.read();
		}
	}

}
